{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-rapidforms": {
      "projectType": "library",
      "root": "projects/i-novation/ng-rapidforms",
      "sourceRoot": "projects/i-novation/ng-rapidforms/src",
      "prefix": "nrf",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/i-novation/ng-rapidforms/tsconfig.lib.json",
            "project": "projects/i-novation/ng-rapidforms/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/i-novation/ng-rapidforms/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/i-novation/ng-rapidforms/src/test.ts",
            "tsConfig": "projects/i-novation/ng-rapidforms/tsconfig.spec.json",
            "karmaConfig": "projects/i-novation/ng-rapidforms/karma.conf.js"
          }
        }
      }
    },
    "test": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/test",
      "sourceRoot": "projects/test/src",
      "prefix": "tst",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test",
            "index": "projects/test/src/index.html",
            "main": "projects/test/src/main.ts",
            "polyfills": "projects/test/src/polyfills.ts",
            "tsConfig": "projects/test/tsconfig.app.json",
            "assets": [
              "projects/test/src/favicon.ico",
              "projects/test/src/assets"
            ],
            "styles": [
              "projects/test/src/styles.scss",
              "node_modules/bootstrap/dist/css/bootstrap.min.css"
            ],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/test/src/environments/environment.ts",
                  "with": "projects/test/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "test:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/test/src/test.ts",
            "polyfills": "projects/test/src/polyfills.ts",
            "tsConfig": "projects/test/tsconfig.spec.json",
            "karmaConfig": "projects/test/karma.conf.js",
            "assets": [
              "projects/test/src/favicon.ico",
              "projects/test/src/assets"
            ],
            "styles": [
              "projects/test/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/test/e2e/protractor.conf.js",
            "devServerTarget": "test:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "ng-rapidforms"
}
